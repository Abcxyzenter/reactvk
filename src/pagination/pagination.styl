.paginationWrapper
  position fixed
  top 0px
  bottom 0px
  margin auto
  right 20px
  width 80px
  height 70vh
  max-height 500px
  background rgba(255,255,255,0.95)
  box-shadow 0px 0px 2px #bbb
  border-radius 24px
  padding 20px 0px
  box-sizing border-box
  overflow hidden
  border-top 2px solid #61dafb
  border-bottom 2px solid #61dafb
  z-index 99

.paginationWrapperInner
  overflow auto
  height calc(100% - 50px)
  text-align center
  position relative


.pageLink
    width 30px
    height 30px
    line-height 28px
    color #555
    font-weight 600
    text-align center
    background white
    border-radius 50%
    margin 5px auto
    cursor pointer
    border 2px solid #61dafb
    & a
      text-decoration none
      color #555

.pageLinkActive
    width 30px
    height 30px
    line-height 28px
    font-weight 600
    text-align center
    background #61dafb
    border-radius 50%
    margin 5px auto
    cursor pointer
    color #fff
    border 2px solid #61dafb
    & a
      text-decoration none
      color #fff